#include<iostream>
# define max 100000
using namespace std;
int main()
{
    unsigned long n,i;
    unsigned long arr[max],arr1[max],b[max];
    cin>>n;
    for(i=0;i<n;i++)
    cin>>arr[i];
    for(i=0;i<n;i++)
    {
     b[i]=0;               
    while(arr[i]>0)
    {
    arr[i]=arr[i]/5;
    b[i]=b[i]+arr[i];          
    }
    arr1[i]=b[i];
    }
    for(i=0;i<n;i++)
    cout<<arr1[i]<<"\n";
    system("pause");
}
     
     
     
     
